@model EmployeeSystem.Models.Employee

<div>
    <dl class="row">
        <dt class="col-sm-3">DateOfJoining</dt>
        <dd class="col-sm-9">@Model.DateOfJoining</dd>

        <dt class="col-sm-3">Branch</dt>
        <dd class="col-sm-9">@Model.BranchName</dd>

                
        <dt class="col-sm-3">Employment Type</dt>
        <dd class="col-sm-9">@Model.EmploymentType</dd>

        <dt class="col-sm-3">Qualification</dt>
        <dd class="col-sm-9">@Model.Qualification</dd>

        
        <dt class="col-sm-3">Experience</dt>
        <dd class="col-sm-9">@Model.Experience</dd>


    </dl>

    <div>
        <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.EmployeeId">Edit Employee</a>
    </div>

    <hr/>
    <div>
        <a asp-action="Index">Back to List</a>
    </div>

</div>
